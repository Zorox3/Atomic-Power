public class AtomicGenerator extends AtomBlock{

	public AtomicGenerator(String name, Material material) {
		super(name, material);
	}

	
	public int getRenderType(){
		return -1;
	}
	
	public boolean isOpaqueCube(){
		return false;
	}
	
}
